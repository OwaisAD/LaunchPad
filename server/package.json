{
  "name": "launchpad-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev:all": "concurrently \"npm run dev:webhook\" \"npm run dev\"",
    "dev:all2": "concurrently \"npm run dev:webhook\" \"npm run start\"",
    "dev": "NODE_ENV=development nodemon src/index.ts",
    "dev:webhook": "ngrok http --url=ruling-dolphin-utterly.ngrok-free.app 8080",
    "dev:staging": "tsx --watch --env-file .env.staging src/index.ts",
    "dev:production": "tsx --watch --env-file .env.production src/index.ts",
    "start": "node --env-file .env dist/src/index.js",
    "start:prod": "npx prisma db push --accept-data-loss && NODE_ENV=production node src/index.js",
    "build": "tsc && mkdir -p dist/src/templates && cp -r src/templates/* dist/src/templates",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ignore-pattern src/templates/**",
    "lint:fix": "eslint src --fix --ignore-pattern src/templates/**",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:migrate": "npx dotenv -e .env.test -- npx prisma migrate reset -f",
    "test": "npm run test:migrate && jest --runInBand --verbose --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.21",
    "jest": "^29.7.0",
    "npm-force-resolutions": "^0.0.10",
    "prisma": "^6.7.0",
    "ts-jest": "^29.3.4",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^4.13.23",
    "@clerk/express": "^1.4.17",
    "@octokit/rest": "^21.1.1",
    "@prisma/client": "^6.7.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.0",
    "cookie": "^1.0.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "express": "^5.1.0",
    "fs-extra": "^11.3.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "libsodium-wrappers": "^0.7.15",
    "morgan": "^1.10.0",
    "redis": "^5.0.1",
    "simple-git": "^3.27.0",
    "slugify": "^1.6.6",
    "svix": "^1.65.0",
    "winston": "^3.17.0",
    "zod": "^3.24.4"
  }
}
